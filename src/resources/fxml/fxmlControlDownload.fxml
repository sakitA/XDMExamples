<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<TitledPane maxHeight="-Infinity" maxWidth="-Infinity" text="%dwn_title" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="xdmexamples.controller.FxmlControlDownloadController">
   <content>
      <VBox spacing="7.0">
         <children>
            <HBox alignment="CENTER_LEFT" spacing="25.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="%dwn_txt1" HBox.hgrow="NEVER">
                     <font>
                        <Font name="Times New Roman Bold" size="18.0" />
                     </font>
                  </Text>
                  <Text fx:id="fileAddress" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" HBox.hgrow="ALWAYS">
                     <font>
                        <Font name="Times New Roman" size="18.0" />
                     </font>
                  </Text>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" maxHeight="30.0" spacing="25.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="%dwn_txt2" HBox.hgrow="NEVER">
                     <font>
                        <Font name="Times New Roman Bold" size="18.0" />
                     </font>
                  </Text>
                  <StackPane HBox.hgrow="ALWAYS">
                     <children>
                        <ProgressBar fx:id="prgBar" prefHeight="18.0" prefWidth="473.0" progress="0.0" HBox.hgrow="ALWAYS" StackPane.alignment="CENTER_LEFT" />
                        <Text fx:id="percentage" strokeType="OUTSIDE" strokeWidth="0.0" text="0%">
                           <font>
                              <Font name="Times New Roman Bold" size="14.0" />
                           </font>
                        </Text>
                     </children>
                  </StackPane>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" maxHeight="30.0" spacing="25.0">
               <children>
                  <HBox alignment="CENTER_LEFT" spacing="25.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="%dwn_txt3" HBox.hgrow="NEVER">
                           <font>
                              <Font name="Times New Roman Bold" size="18.0" />
                           </font>
                        </Text>
                        <Text fx:id="dwnFileSize" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" HBox.hgrow="ALWAYS">
                           <font>
                              <Font name="Times New Roman" size="18.0" />
                           </font>
                        </Text>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" spacing="25.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="%dwn_txt4" HBox.hgrow="NEVER">
                           <font>
                              <Font name="Times New Roman Bold" size="18.0" />
                           </font>
                        </Text>
                        <Text fx:id="fileSize" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" HBox.hgrow="ALWAYS">
                           <font>
                              <Font name="Times New Roman" size="18.0" />
                           </font>
                        </Text>
                     </children>
                  </HBox>
               </children></HBox>
            <HBox alignment="CENTER" spacing="15.0">
               <children>
                  <Button fx:id="btnStart" minWidth="130.0" mnemonicParsing="false" onAction="#dwnStart" text="%dwn_btn1">
                     <font>
                        <Font name="Times New Roman Bold" size="18.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnPause" disable="true" minWidth="130.0" mnemonicParsing="false" onAction="#dwnPause" text="%dwn_btn2">
                     <font>
                        <Font name="Times New Roman Bold" size="18.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnResume" disable="true" minWidth="130.0" mnemonicParsing="false" onAction="#dwnResume" text="%dwn_btn3">
                     <font>
                        <Font name="Times New Roman Bold" size="18.0" />
                     </font>
                  </Button>
                  <Button fx:id="btnStop" disable="true" minWidth="130.0" mnemonicParsing="false" onAction="#dwnStop" text="%dwn_btn4">
                     <font>
                        <Font name="Times New Roman Bold" size="18.0" />
                     </font>
                  </Button>
               </children></HBox>
         </children>
      </VBox>
   </content>
   <font>
      <Font name="Times New Roman Bold" size="18.0" />
   </font>
</TitledPane>
