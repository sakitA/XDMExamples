<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="xdmexamples.controller.FxmlDownloadController">
   <children>
      <HBox alignment="CENTER" spacing="30.0" VBox.vgrow="NEVER">
         <children>
            <TextField fx:id="urlText" maxWidth="1.7976931348623157E308" prefHeight="25.0" prefWidth="360.0" promptText="%url_txt" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets left="15.0" />
               </HBox.margin>
               <font>
                  <Font name="Times New Roman" size="18.0" />
               </font>
            </TextField>
            <Button fx:id="btnDownload" disable="true" minHeight="35.0" minWidth="170.0" mnemonicParsing="false" onAction="#download" text="%btn_dwn" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <font>
                  <Font name="Times New Roman Bold" size="18.0" />
               </font>
            </Button>
            <Button fx:id="btnCancel" disable="true" minHeight="35.0" minWidth="170.0" mnemonicParsing="false" onAction="#cancelAll" text="%btn_cnl" HBox.hgrow="NEVER">
               <font>
                  <Font name="Times New Roman Bold" size="18.0" />
               </font>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
            <ProgressIndicator fx:id="prgIndicator" maxHeight="30.0" maxWidth="30.0" visible="false" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets right="15.0" />
               </HBox.margin>
            </ProgressIndicator>
         </children>
      </HBox>
      <VBox fx:id="vbox" spacing="5.0" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
